{
  "manifest_version": 2,

  "name": "TabIt",
  "description": "Extension to save all the open tabs in your window for a later time",
  "version": "1.1",

  "browser_action": {
    "default_icon": "TabIt_128.png",
    "default_popup": "popup.html",
    "default_title": "TabIt"
  },

  "background": {
    "scripts": []
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "script.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "https://ajax.googleapis.com/"
  ]

}